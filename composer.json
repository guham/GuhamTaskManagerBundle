{
    "name": "guham/task-manager-bundle",
    "type": "symfony-bundle",
    "description": "Task manager with Symfony 4",
    "keywords": ["task", "manager", "todo", "todo-list"],
    "homepage": "https://github.com/guham/GuhamTaskManagerBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Guillaume Hamoniez",
            "email": "guham.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "doctrine/annotations": "^1.6",
        "doctrine/orm": "^2.6",
        "easycorp/easyadmin-bundle": "^1.17",
        "stof/doctrine-extensions-bundle": "^1.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.10",
        "phpunit/phpunit": "^6.1",
        "symfony/framework-bundle": "^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/yaml": "^4.0"
    },
    "autoload": {
        "psr-4": { "Guham\\TaskManagerBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Guham\\TaskManagerBundle\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
